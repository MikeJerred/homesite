{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/server/main.ts",
			"protocol": "inspector",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/dist",
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"NPM_CONFIG_PRODUCTION": "false"
			},
			"console": "integratedTerminal",
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/dist/**"]
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/dist/**"],
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}